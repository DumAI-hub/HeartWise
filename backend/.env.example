# Copy this file to `.env` in the same folder and fill in real values.

# Database configuration
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=your-password-here
DB_NAME=

# ML Service URL (Python microservice)
ML_SERVICE_URL=http://localhost:8000

# ML model paths (to be filled later)
MODEL1_PATH=/path/to/your/first/base/model
MODEL2_PATH=/path/to/your/second/base/model
MODEL3_PATH=/path/to/your/third/base/model
STACKING_MODEL_PATH=/path/to/your/stacking/model

# Gemini API key (rotate if previously exposed)
GEMINI_API_KEY=your_gemini_api_key_here
